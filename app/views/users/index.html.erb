<table>
  <thead>
    <tr>
      <th>姓</th>
      <th>名</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
    <tr>
      <td><%= user.last_name %></td>
      <td><%= user.first_name %></td>
      <td><%= link_to '詳細', user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>